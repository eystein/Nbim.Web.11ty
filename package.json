{
	"name": "nbim_web_11ty",
	"version": "1.0.1",
	"description": "Static HTML library for NBIM",
	"private": true,
	"scripts": {
		"watch:sass": "sass --watch --update --style=expanded src/scss:docs/assets/css",
		"build:sass": "sass --no-source-map --style=compressed src/scss:static/css",
		"watch:js": "npx webpack --config-name siteConfig",
		"build:js": "npm run webpack --config-name packageConfig",
		"watch:eleventy": "npx @11ty/eleventy --serve",
		"build:eleventy": "npm @11ty/eleventy",
		"dev": "npm-run-all --parallel watch:*",
		"build": "npm-run-all build:*",
		"test": "npx @11ty/eleventy --dryrun"
	},
	"author": "NoA Ignite",
	"license": "ISC",
	"contributors": [
		{
			"name": "Eystein Mack Aln√¶s"
		},
		{
			"name": "Knut Sorknes"
		},
		{
			"name": "Hanne Edfelt"
		}
	],
	"devDependencies": {
		"@11ty/eleventy": "^1.0.2",
		"npm-run-all": "^4.1.5",
		"sass": "^1.56.0",
		"webpack": "^5.75.0",
		"webpack-cli": "^5.0.0"
	},
	"dependencies": {
		"core-js": "^3.26.1",
		"deepmerge": "^4.2.2",
		"file-saver": "^2.0.5",
		"headroom.js": "^0.12.0",
		"highcharts": "^10.3.1",
		"intersection-observer-polyfill": "^0.1.0",
		"regenerator-runtime": "^0.13.11",
		"terser": "^5.15.1"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/makingwaves/noa-nbim-frontend.git"
	},
	"publishConfig": {
		"registry": "https://npm.pkg.github.com/"
	},
	"files": [
		"static/*"
	]
}
